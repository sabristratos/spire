{
    "name": "spire-ui",
    "version": "1.1.1",
    "description": "A modern TALL stack component library with Tailwind v4, Livewire 3, and Alpine.js",
    "type": "module",
    "main": "./dist/spire-ui.iife.js",
    "module": "./dist/spire-ui.esm.js",
    "style": "./dist/spire-ui.css",
    "exports": {
        ".": {
            "import": "./dist/spire-ui.esm.js",
            "require": "./dist/spire-ui.iife.js"
        },
        "./dist/*": "./dist/*",
        "./css": "./dist/spire-ui.css"
    },
    "files": [
        "dist",
        "resources/views",
        "resources/lang"
    ],
    "scripts": {
        "build": "vite build",
        "dev": "vite build --watch"
    },
    "keywords": [
        "laravel",
        "livewire",
        "tailwind",
        "alpine",
        "components",
        "ui",
        "tall-stack"
    ],
    "license": "MIT",
    "devDependencies": {
        "vite": "^7.0.0",
        "@tailwindcss/vite": "^4.0.0"
    },
    "dependencies": {
        "flag-icons": "^7.2.3",
        "@oddbird/css-anchor-positioning": "^0.7.0",
        "@oddbird/popover-polyfill": "^0.6.1",
        "@tiptap/core": "^2.0.0",
        "@tiptap/starter-kit": "^2.0.0",
        "@tiptap/extension-link": "^2.0.0",
        "@tiptap/extension-image": "^2.0.0",
        "@tiptap/extension-placeholder": "^2.0.0",
        "@tiptap/extension-character-count": "^2.0.0"
    },
    "peerDependencies": {
        "alpinejs": "^3.14.0",
        "tailwindcss": "^4.0.0"
    }
}
