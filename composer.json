{
    "name": "stratos/spire-ui",
    "description": "A modern TALL stack component library with Tailwind v4, Livewire 3, and Alpine.js",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "livewire",
        "tailwind",
        "alpine",
        "components",
        "ui",
        "tall-stack"
    ],
    "homepage": "https://sabristratos.github.io/spire/",
    "support": {
        "issues": "https://github.com/sabristratos/spire/issues",
        "source": "https://github.com/sabristratos/spire",
        "docs": "https://sabristratos.github.io/spire/"
    },
    "authors": [
        {
            "name": "Sabri Kallel",
            "email": "sabri@stratosdigital.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^12.0",
        "livewire/livewire": "^3.0",
        "symfony/process": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "SpireUI\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "SpireUI\\SpireUIServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.1.11"
}
